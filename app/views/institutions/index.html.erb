<h1>Listing institutions</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Zipcode</th>
    <th>Name</th>
    <th>Industry code</th>
    <th>Description</th>
    <th>Path to logo</th>
    <th>Street 1</th>
    <th>Street 2</th>
    <th>Phone</th>
    <th>Linkedin</th>
    <th>Email extention</th>
    <th>Active</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @institutions.each do |institution| %>
  <tr>
    <td><%= institution.id %></td>
    <td><%= institution.zipcode %></td>
    <td><%= institution.name %></td>
    <td><%= institution.industry_code %></td>
    <td><%= institution.description %></td>
    <td><%= institution.path_to_logo %></td>
    <td><%= institution.street_1 %></td>
    <td><%= institution.street_2 %></td>
    <td><%= institution.phone %></td>
    <td><%= institution.linkedin_id %></td>
    <td><%= institution.email_extention %></td>
    <td><%= institution.active %></td>
    <td><%= link_to 'Show', institution %></td>
    <td><%= link_to 'Edit', edit_institution_path(institution) %></td>
    <td><%= link_to 'Destroy', institution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Institution', new_institution_path %>
